<h1>Listing group_questions</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @group_questions.each do |group_question| %>
      <tr>
        <td><%= group_question.name %></td>
        <td><%= link_to 'Show', group_question %></td>
        <td><%= link_to 'Edit', edit_group_question_path(group_question) %></td>
        <td><%= link_to 'Destroy', group_question, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Group question', new_group_question_path %>
